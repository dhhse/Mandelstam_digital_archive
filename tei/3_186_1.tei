    <text>
        <body>
            <div1 n="2" type="volume">
                <div2 n="63" type="poem" rhyme="aabbccddeeffaaff">
                    <head>ЧАРЛИ ЧАПЛИН</head>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="1">ЧАРЛИ ЧАПЛИН</l>
                        <l n="2">Чарли Чаплин</l>
                        <l n="3">Две подметки,</l>
                        <l n="4">Две гляделки,</l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="5">И прекрасных</l>
                        <l n="6">Чарли Чаплин — </l>
                        <l n="7">Две подметки — </l>
                        <l n="8">Как-то мы живем неладно все — </l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="9">Оловянный</l>
                        <l n="10">Голова не</l>
                        <l n="11">Ходит сажа,</l>
                        <l n="12">И тихонько</l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="13">Для чего я славен и любим</l>
                        <l n="14">И ведет его шоссе большое</l>
                        <l n="15">Чарли Чаплин,</l>
                        <l n="16">Чаплин, кролик,</l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="17">Чисти корольки,</l>
                        <l n="18">А твоя жена — </l>
                        <l n="19">И чудит, чудит</l>
                        <l n="20">Отчего</l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="21">Почему</l>
                        <l n="22">Потому — </l>
                        <l n="23">Чарли, Чарли, — </l>
                        <l n="24">Ты совсем</l>
                    </lg>
                    <lg type="quatrain" rhyme="" met="">
                        <l n="25">Котелок твой — </l>
                        <l n="26">А Москва</l>
                        <l n="27">В дорогую дорогу.</l>
                        <l n="28">Май(?) 1937</l>
                    </lg>
                </div2>
            </div1>
        </body>
